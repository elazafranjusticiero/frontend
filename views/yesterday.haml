%ul
  - errors.each do |error|
    %li 
      %h3= error.timestamp.strftime('%a %-d, %T')
      %dl
        %dt Message: 
        %dd
          %a{:href => '/occurrences?message=' + error.message}= error.message
        %dt File: 
        %dd
          %a{:href => '/occurrences?file=' + error.file}= error.file